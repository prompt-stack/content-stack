{
  "id": "workflow-content-pipeline",
  "type": "workflow",
  "created_at": "2025-01-20T00:00:00.000Z",
  "updated_at": "2025-01-20T00:00:00.000Z",
  "data": {
    "name": "Content Processing Pipeline",
    "description": "Default workflow for processing new content",
    "status": "active",
    "trigger": "content_added",
    "steps": [
      {
        "id": "validate",
        "type": "validation",
        "config": {
          "checkDuplicates": true,
          "validateFormat": true,
          "maxSize": 10485760
        }
      },
      {
        "id": "extract",
        "type": "extraction",
        "config": {
          "extractText": true,
          "extractMetadata": true,
          "generateThumbnail": true
        }
      },
      {
        "id": "enrich",
        "type": "enrichment",
        "config": {
          "enabled": false,
          "model": "gpt-4",
          "prompt": "INBOX_PROCESSING.md",
          "generateTitle": true,
          "generateCategory": true,
          "generateTags": true,
          "generateSummary": true
        }
      },
      {
        "id": "store",
        "type": "storage",
        "config": {
          "organize": true,
          "compress": false,
          "encrypt": false
        }
      }
    ],
    "errorHandling": {
      "retries": 3,
      "notifyOnError": true,
      "fallbackWorkflow": null
    }
  }
}